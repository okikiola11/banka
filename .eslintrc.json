{
    "extends": ["airbnb-base", "prettier"],
    "plugins": [
        "prettier"
    ],
    "env": {
        "node": true,
        "browser": true
    },
    "rules": {
        "prettier/prettier": [
            "error"
        ],
        "linebreak-style": [
            "error",
            "windows"
        ]
    }
}